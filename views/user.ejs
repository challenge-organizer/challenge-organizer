<%- include('partials/header') %> 
<%- include('partials/nav') %> 
<div class="container">
  <div>
		<br>
		<h1 class="display-2">Data User</h1>
    <br>
  </div>
  <div>
    <button class="btn btn-info"><a href="/challenge/add">Add Challenge</a></button>
    <table class="table table-hover">
      <tr>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Action</th>
      </tr>
      <%user.forEach((el)=>{%>
        <tr>
            <td><%=el.first_name%></td>
            <td><%=el.last_name%></td>
          <td>
          <button class="btn btn-success"><a href="/user/<%=el.id%>/giveScore">Edit</a></button>
          </td>
        </tr>
      <%})%>
    </table>
  </div>
</div>
<%- include('partials/footer') %> 